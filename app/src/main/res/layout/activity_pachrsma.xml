<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".pachrsma" tools:layout_editor_absoluteY="81dp">

    <TextView
            android:text="PA CH RS MA"
            android:layout_width="352dp"
            android:layout_height="41dp"
            android:id="@+id/textView14" android:textSize="30sp"
            app:layout_constraintStart_toEndOf="@+id/imageView15" app:layout_constraintEnd_toEndOf="@+id/imageView15"
            app:layout_constraintBottom_toTopOf="@+id/_dynamic" app:layout_constraintHorizontal_bias="0.497"/>
    <ListView
            android:layout_width="384dp"
            android:layout_height="0dp"
            app:layout_constraintTop_toBottomOf="@+id/button6"
            app:layout_constraintStart_toEndOf="@+id/imageView15" android:layout_marginBottom="16dp"
            app:layout_constraintEnd_toEndOf="@+id/imageView15" app:layout_constraintBottom_toBottomOf="parent"/>
    <Button
            android:text="Pesquisar"
            android:layout_width="111dp"
            android:layout_height="41dp"
            android:id="@+id/button6"
            android:layout_marginBottom="157dp" app:layout_constraintEnd_toEndOf="@+id/textInputLayout"
            app:layout_constraintBottom_toTopOf="@+id/_dynamic" app:layout_constraintTop_toTopOf="parent"
            android:layout_marginTop="28dp" android:layout_marginEnd="4dp" android:layout_marginRight="4dp"/>
    <ImageView
            android:layout_width="384dp"
            android:layout_height="99dp" app:srcCompat="?attr/actionModeSplitBackground"
            android:id="@+id/imageView11"
            app:layout_constraintStart_toEndOf="@+id/imageView15" app:layout_constraintEnd_toEndOf="@+id/imageView15"
            app:layout_constraintBottom_toTopOf="@+id/imageView15" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.497"/>
    <android.support.design.widget.TextInputLayout
            android:layout_width="366dp"
            android:layout_height="74dp"
            android:id="@+id/textInputLayout"
            app:layout_constraintBottom_toBottomOf="@+id/imageView11"
            app:layout_constraintStart_toEndOf="@+id/imageView15" app:layout_constraintEnd_toEndOf="@+id/imageView15"
            app:layout_constraintHorizontal_bias="0.497">

        <android.support.design.widget.TextInputEditText
                android:layout_width="246dp"
                android:layout_height="wrap_content"
                android:background="@android:color/background_light"/>
    </android.support.design.widget.TextInputLayout>
    <ImageView
            android:layout_width="175dp"
            android:layout_height="0dp" app:srcCompat="@mipmap/pachrs"
            android:id="@+id/imageView15"
            app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
            android:layout_marginStart="16dp" app:layout_constraintTop_toBottomOf="@+id/imageView11"
            android:layout_marginBottom="36dp" app:layout_constraintBottom_toTopOf="@+id/_dynamic"/>
</android.support.constraint.ConstraintLayout>